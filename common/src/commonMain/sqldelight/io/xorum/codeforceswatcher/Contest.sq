CREATE TABLE IF NOT EXISTS DbContest (
    id INTEGER NOT NULL,
    name TEXT NOT NULL,
    startTimeSeconds INTEGER NOT NULL,
    durationSeconds INTEGER NOT NULL,
    phase TEXT NOT NULL,
    PRIMARY KEY (id)
);

getAll:
SELECT * FROM DbContest;

insert:
INSERT OR REPLACE INTO DbContest(id, name, startTimeSeconds, durationSeconds, phase) VALUES(?,?,?,?,?);

deleteAll:
DELETE FROM DbContest;